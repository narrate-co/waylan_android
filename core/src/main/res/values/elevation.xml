<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- ELEVATION -->
    <dimen name="plane_00">@dimen/m3_sys_elevation_level0</dimen>
    <dimen name="plane_01">@dimen/m3_sys_elevation_level1</dimen>
    <dimen name="plane_02">@dimen/m3_sys_elevation_level2</dimen>
    <dimen name="plane_03">@dimen/m3_sys_elevation_level3</dimen>
    <dimen name="plane_04">@dimen/m3_sys_elevation_level4</dimen>
    <dimen name="plane_05">@dimen/m3_sys_elevation_level5</dimen>

    <!-- TRANSLUCENCE -->
    <!--Lowest surfaces that have a fully opaque background and are-->
    <!--usually paired with plane_00 -->
    <item name="translucence_00" type="dimen" format="float">1.0</item>

    <!--Elevated surfaces that wish to express elevation through-->
    <!--transparency rather than elevation/shadows. Typically paired -->
    <!--with plane_01-->
    <item name="translucence_01" type="dimen" format="float">0.8</item>

</resources>